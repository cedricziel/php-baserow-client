{
    "name": "cedricziel/baserow",
    "description": "An API client for baserow.io",
    "type": "library",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "cedricziel\\phpbaserowclient\\": "src/",
            "cedricziel\\phpbaserowclient\\Generated\\": "generated/"
        }
    },
    "authors": [
        {
            "name": "Cedric Ziel",
            "email": "mail@cedric-ziel.com"
        }
    ],
    "require-dev": {
        "jane-php/open-api-3": "^7.8",
        "friendsofphp/php-cs-fixer": "^3.59"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require": {
        "php": "^8.3",
        "jane-php/open-api-runtime": "^7.8"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        }
    }
}
